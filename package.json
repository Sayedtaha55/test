{
  "name": "ray-test-marketplace",
  "version": "1.0.0",
  "description": "Hyper-modern multi-vendor marketplace for Egypt.",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "backend:dev": "tsx watch --tsconfig backend/tsconfig.json backend/main.ts",
    "backend:start": "tsx --tsconfig backend/tsconfig.json backend/main.ts",
    "backend:render:start": "prisma generate --schema prisma/schema.postgres.prisma && prisma db push --schema prisma/schema.postgres.prisma && tsx --tsconfig backend/tsconfig.json backend/main.ts",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:push": "prisma db push --schema prisma/schema.prisma",
    "prisma:generate:postgres": "prisma generate --schema prisma/schema.postgres.prisma",
    "prisma:push:postgres": "prisma db push --schema prisma/schema.postgres.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "build": "vite build",
    "preview": "vite preview",
    "start": "npm run dev",
    "vercel-build": "npm run build"
  },
  "keywords": [
    "react",
    "marketplace",
    "egypt",
    "ai",
    "gemini",
    "pos"
  ],
  "author": "Ray Team",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.0.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-express": "^11.1.11",
    "@prisma/client": "^6.19.1",
    "@types/morgan": "^1.9.10",
    "@types/passport-jwt": "^4.0.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^7.2.7",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "express-rate-limit": "^7.1.5",
    "express-winston": "^4.2.0",
    "framer-motion": "^12.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.400.0",
    "morgan": "^1.10.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0",
    "recharts": "^3.6.0",
    "prisma": "^6.19.1",
    "sharp": "^0.33.5",
    "tsx": "^4.20.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "jest": "^30.2.0",
    "postcss": "^8.5.6",
    "supertest": "^7.2.2",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
